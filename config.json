{
    "settings": {
        "data_root": "data",
        "preprocess": false,
        "encode": false
    },
    "data": {
        "data_monthly": "mlsruod_monthly.csv",
        "data_yearly": "mlsruod_yearly.csv"
    },
    "train": {
        "model_path": "models",
        "model_save_path": "models/trained",
        "epochs": 
        50,
        "ensamble_num": 3,
        "batch_size": 512
    },
    "evaluation": {
        "results_root": "results",
        "evaluation_csv": "resul-ts/evaluation_results.csv",
        "ensamble_log_csv": "results/ensamble_log.csv",
        "model_log_metrics": [
            "MAE",
            "MSE",
            "OSS",
            "RSS",
            "TSS",
            "OSS_theory"
        ],
        "training_width": 1,
        "validation_width": 1,
        "testing_width": 1

    },
    "model": {
        "loss": "mse",
        "metrics": [
            "mse",
            "mape",
            "mae",
            "msle"
        ],
        "optimizer": "adam",
        "hidden_layer": 4,
        "num_units": 600,
        "learning_rate": 0.001
    }
}