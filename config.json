{
    "settings": {
        "data_root": "data",
        "preprocess": false
    },
    "data": {
        "data_yearly": "mlsruod_yearly.csv",
        "data_monthly": "mlsruod_monthly.csv"
    },
    "train": {
        "model_path": "models",
        "checkpoint_path": "models/checkpoints",
        "epochs": 20
    },
    "evaluation": {
        "results_root": "results",
        "evaluation_csv": "results/evaluation_results.csv"
    },
    "model": {
        "available_losses": [
            "MeanSquaredError",
            "MeanAbsolutePercentageError",
            "MeanAbsoluteError",
            "MeanSquaredLogarithmicError"
        ],
        "loss": "mse",
        "metrics": [
            "mse",
            "mape",
            "mae",
            "msle"
        ],
        "optimizer": "adam",
        "input_dim": 935,
        "hidden_layer": 5,
        "num_units": 600,
        "learning_rate": 0.001
    }
}
